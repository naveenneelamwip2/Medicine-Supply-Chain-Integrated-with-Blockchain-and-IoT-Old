{
  "name": "medical-supply-chain-integrated-with-blockchain-and-iot",
  "version": "1.0.0",
  "description": "Truthfulness of data is essential in every supplychain application hence this project contains a blockchain application designed to enhance the transparency and security of drug supply chain management. By integrating Internet of Things (IoT) devices, we ensure real-time and accurate tracking of drug delivery processesâ€”from production to final delivery",
  "main": "index.ts",
  "scripts": {
    "webpack": "webpack --config webpack.config.js",
    "webpack:watch": "webpack --config webpack.config.js --watch",
    "lint": "tslint **/*.ts --exclude node_modules/",
    "build": "npm run webpack",
    "start": "cross-env NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec 'ts-node' -r tsconfig-paths/register ./src/index.ts",
    "dev": "npm run start",
    "deploy": "cross-env NODE_ENV=production npm run build",
    "test:unit": "cross-env NODE_ENV=test jest --config jest.config.spec.js --runInBand",
    "test:e2e": "npm run build && cross-env NODE_ENV=test jest --config jest.config.e2e.js --runInBand",
    "test": "npm run test:unit && npm run test:e2e"
  },
  "author": "",
  "license": "",
  "dependencies": {
    "@web3-storage/w3up-client": "^16.0.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.1",
    "fortjs": "^2.7.0",
    "fortjs-passport": "^1.0.0",
    "keccak256": "^1.0.6",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^18.18.0",
    "@types/passport-local": "^1.0.38",
    "axios": "^0.19.0",
    "cross-env": "^5.2.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^5.11.0",
    "typescript": "^5.1.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "project": {
    "framework": "fortjs",
    "language": "typescript"
  }
}
